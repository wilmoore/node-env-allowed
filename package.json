{
  "name": "env-allowed",
  "version": "0.1.0",
  "keywords": [
    "allow",
    "allowed",
    "env",
    "envc",
    "environments",
    "environment",
    "variables",
    "variable",
    "12factor"
  ],
  "description": "Whether an environment variable's value is contained within an allow list. Allow list is provided by a comma-separated environment variable value.",
  "author": "Wil Moore III <wil.moore@wilmoore.com>",
  "contributors": [
    {
      "name": "Wil Moore III",
      "email": "wil.moore@wilmoore.com"
    }
  ],
  "watches": [
    {
      "script": "test",
      "patterns": [
        "*.js",
        "lib/**/*.js",
        "test/**/*.js"
      ]
    },
    {
      "script": "lint",
      "patterns": [
        "*.js",
        "lib/**/*.js",
        "test/**/*.js"
      ]
    }
  ],
  "scripts": {
    "lint": "jshint *.js",
    "test": "node test.js test/**/*.js | faucet",
    "watch": "rerun-script"
  },
  "files": [
    "*.json",
    "*.js",
    "license",
    "readme.md"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wilmoore/node-env-allowed"
  },
  "bugs": {
    "url": "https://github.com/wilmoore/node-env-allowed/issues"
  },
  "devDependencies": {
    "faucet": "0.0.1",
    "jshint": "^2.5.10",
    "rerun-script": "^0.4.1",
    "tape": "^3.0.3"
  },
  "dependencies": {
    "env-accessors": "^0.2.1",
    "to-array.js": "^0.10.0"
  },
  "config": {},
  "license": "MIT"
}
